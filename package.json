{
  "name": "us-state-flags",
  "version": "1.0.1",
  "description": "Complete US state data with flags, utility functions, and React component - zero dependencies for data, optional React support - SVG-only flags, offline ready",
  "main": "src/index.js",
  "module": "src/index.mjs",
  "browser": "src/index.browser.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs",
      "require": "./src/index.js",
      "browser": "./src/index.browser.js",
      "default": "./src/index.js"
    },
    "./components": {
      "types": "./src/components/index.d.ts",
      "import": "./src/components/index.js",
      "require": "./src/components/index.js",
      "default": "./src/components/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepublishOnly": "node -e \"console.log('âœ… Package ready for publish')\""
  },
  "keywords": [
    "usa",
    "states",
    "flags",
    "geography",
    "data",
    "territories",
    "capitals",
    "timezones",
    "browser",
    "nodejs",
    "typescript",
    "tree-shaking",
    "zero-dependencies",
    "universal",
    "react",
    "component",
    "jsx",
    "ui",
    "svg-only",
    "offline-ready",
    "scalable-vector",
    "lightweight"
  ],
  "author": "Foxsense Innovations",
  "homepage": "https://foxsense-innovations.github.io/us-state-flags-home-page/",
  "license": "ISC",
  "files": [
    "src/data/",
    "src/assets/flags/svg/",
    "src/index.*",
    "src/components/",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
